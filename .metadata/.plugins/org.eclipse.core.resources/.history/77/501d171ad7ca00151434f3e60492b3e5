import java.awt.Window;

import javax.swing.JFrame;

public class Screen {
GraphicsDevice gd;
public Screen(){
	GraphicsEnvironment gvn = GraphicsEnvironment.getLocalGraphicsEnvironment();
	gd = gvn.getDefaultScreenDevice();
	
}
}
