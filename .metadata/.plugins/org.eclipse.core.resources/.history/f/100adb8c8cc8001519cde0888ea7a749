import java.awt.Graphics;

public class Entity {
	protected int x ,y, w ,h;
	public int getX() {
		return x;
	}
	
	public int getY() {
		return y;
	}
	
	public int getW() {
		return w;
	}
	
	public int getH() {
		return h;
	}
	
	protected boolean remove = false;
	public Entity(int x , int y){
		this.x = x;
		this.y = y;
	}
	public void draw(Graphics g){
		
	}
	

}
