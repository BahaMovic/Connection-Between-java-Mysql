package test;

public class test {
	public int x;
	public test(){};
	public test(int x){this.x = x;}
	public int backint(){return x;} 
	public void count(){x++;}
	public void less(){x--;}
	public void increment(int i){x += i;}
	public static void main(String[] args){
		test t = new test();
		System.out.println(t.x);
		t = new test(12);
	}

}
